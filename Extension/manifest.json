{
	"manifest_version": 2,
	"name": "Addic8ed",
	"description": "Provide enhancement to Addic7ed.",
	"version": "0.0.1",
	"permissions": [
		"http://www.addic7ed.com/"
	],
	"icons": {
		"128": "icon_128.png"
	},
	"content_scripts": [{
		"matches": ["http://www.addic7ed.com/*"],
		"run_at": "document_start",
		"css": ["addic8ed.css"],
		"js": ["jquery-2.1.1.min.js", "addic8ed.js"]
	}],
	"web_accessible_resources": [
		"web/*"
	]
}